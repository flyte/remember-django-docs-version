{
    "manifest_version": 2,
    "name": "Remember Django Docs Version",
    "version": "0.0.1",
    "description": "Remembers which version of Django docs you've selected",
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "js": ["docs_content.js"],
        "matches": ["https://docs.djangoproject.com/*"]
    }],
    "browser_action": {
        "default_popup": "popup.html"
    }
  }